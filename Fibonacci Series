
class Main{  
 static int num1=0,num2=1,num3=0;    
 static void FibonacciSeries(int c){    
    if(c>0){    
         num3 = num1 + num2;    
         num1 = num2;    
         num2 = num3;    
         System.out.print(" "+num3);   
         FibonacciSeries(c-1);    
     }    
 }    
 public static void main(String args[]){    
  int c=20;    
  System.out.print(num1+" "+num2);    
  FibonacciSeries(c-2);   
 }  
}
